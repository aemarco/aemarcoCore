<!--EXTERNAL_PROPERTIES: AssemblyVersion-->
<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\SolutionInfo.proj" />

	<!-- project definition -->
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ApplicationIcon>girl.ico</ApplicationIcon>
	</PropertyGroup>

	<!-- nuget build-->
	<PropertyGroup>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>aemarcoPersonCrawler</PackageId>
		<PackageVersion>$(AssemblyVersion)</PackageVersion>
		<Authors>aemarco</Authors>
		<Company>aemarco</Company>
		<Product>aemarcoPersonCrawler</Product>
		<PackageProjectUrl>https://github.com/aemarco/aemarcoCore</PackageProjectUrl>
		<RepositoryUrl>https://github.com/aemarco/aemarcoCore</RepositoryUrl>
		<Description>crawling for person information</Description>
		<PackageTags>Crawler,FSK18</PackageTags>
		<PackageIcon>girl.png</PackageIcon>
		<Copyright>Copyright 2021</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	</PropertyGroup>
	<ItemGroup>
		<None Include="girl.png" Pack="true" PackagePath="\" />
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="Base\" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Update="HtmlAgilityPack" Version="1.11.46" />
	</ItemGroup>
	

</Project>
